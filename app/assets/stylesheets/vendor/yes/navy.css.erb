}
.box.pattern {
    background-color: #f1f5e4;
}
#gallery-wrapper .block .portfolio-hover, #gallery-wrapper .block .video-hover {
	background: rgba(0, 192, 172, 0.7);
}
.overlay {
	background: rgba(230, 211, 101, 0.7);
}
