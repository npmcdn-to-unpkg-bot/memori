h1, h2, h3, h4, h5, h6 {
    color: #00004c;
}
a {
    color: #00004c;
}
a:hover, a:active {
    color: #00004c;
}
#page-content blockquote {
    border-left: 3px solid #00004c;
}
.bgcolor {
    background-color: #00004c;
}
span.fill {
    background-color: #00004c;
}
a.button.fill {
    background: #00004c;
}
.timeline .column.heart:after {
    color: #00004c;
}
.timeline-year span {
    background-color: #00004c;
}
.loaderOverlay i {
    color: #00004c;
}
#logo {
    color: #00004c;
}
