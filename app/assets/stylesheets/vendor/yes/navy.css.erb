	color: #00004c;
}
#invitation a {
    background: #00004c;
}
.social ul li a:hover {
    color: #00004c;
}
.box a:hover {
    color: #00004c;
}
.box .date:before {
    color: #00004c;
}
#twitter .follow:hover {
    color: #00004c;
}
.small-box .amount {
    color: #00004c;
}
.small-box .donate:hover {
    color: #00004c;
}
#instagram-section .hash {
    color: #00004c;
}
.form input[type="submit"] {
    color: #00004c;
}
.form input[type="submit"].color {
    background-color: #00004c;
}
.form input[type="submit"].color:hover {
	background-color: #00004c;
}
.form .column:after {
    color: #00004c;
}
footer .logo span {
    color: #00004c;
}
#back-home {
    background: #00004c;
}
.prev:hover:before,.next:hover:after {
    color: #00004c;
}
.content p:first-child:first-letter {
    color: #00004c;
}
#comments h3.section-title span {
    color: #00004c;
}
#back-to-top {
    background: #00004c;
}
#menu-toggle {
    background: #00004c;
}
