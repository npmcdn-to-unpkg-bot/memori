/* 2.5 - Hexagon
---------------------------------*/
.hex {
	position: relative;
	width: 142px;
	height: 88px;
	border-radius: 10px;
	margin: 85px auto 62px auto;
}
.hex:before,
.hex:after {
	position: absolute;
	width: inherit; height: inherit;
	border-radius: inherit;
	background: inherit;
	content: '';
	margin-left: -1px;
}
.hex ,
.hex:before,
.hex:after {
	border-left: 2px solid #ea6153;
	border-right: 2px solid #ea6153;
	background: #ea6153;
}
.hex:before {
	-webkit-transform: rotate(60deg);
	-moz-transform: rotate(60deg);
	-ms-transform: rotate(60deg);
	-o-transform: rotate(60deg);
	transform: rotate(60deg);
}
.hex:after {
	-webkit-transform: rotate(-60deg);
	-moz-transform: rotate(-60deg);
	-ms-transform: rotate(-60deg);
	-o-transform: rotate(-60deg);
	transform: rotate(-60deg);
}
.hex.hex-margin {
	margin-bottom: 60px;
}
.hex-icon {
	color: #ffffff;
	font-size: 4.0625em;
	line-height: 85px;
	text-align: center;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 1;
}
.hex,
.hex:after,
.hex:before,
.hex-icon {
	-webkit-transition: all .25s;
	-moz-transition: all .25s;
	-ms-transition: all .25s;
	-o-transition: all .25s;
	transition: all .25s;
}

